$(document).ready(function() {
    $('#graph-container0').highcharts({
        chart: {
            plotBackgroundColor: null,
            plotBorderWidth: null,
            plotShadow: false
        },
        credits: {
            enabled: false
        },
        title: {
            text: '<strong>Form One - Overall Score Analysis - Term Two 2010</strong>'
        },
        tooltip: {
    	    pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
        },
        plotOptions: {
            pie: {
                allowPointSelect: true,
                cursor: 'pointer',
                dataLabels: {
                    enabled: true,
                    format: '<b>{point.name}</b>: {point.percentage:.1f} %',
                    style: {
                        color: (Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black'
                    },
		            showInLegend: true
                }
            }
        },
        series: [{
            type: 'pie',
            name: 'Percentage',
            data: [
                ['A - 26 ',   45.0],
                ['B - 30 ',       26.8],
                {
                    name: 'C - 48 ',
                    y: 12.8,
                    sliced: true,
                    selected: true
                },
                ['D - 52 ',    8.5],
                ['E - 35 ',     6.2]
            ]
        }]
    });

